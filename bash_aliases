#!/bin/bash
# alias for speedlight
alias ..='cd ..'
alias actualizar='sudo apt-get update && sudo apt-get upgrade'
alias ll='ls -ltrh'
alias la='ls -altrh'
alias v='vim'
alias df='df -h'
alias reiniciar='gksu "shutdown -r now"'
alias apagar='gksu "shutdown -h now"'
alias dormir='gksu "pm-suspend"'
alias p3m='python3 manage.py $1'
alias roombox='cd $HOME/SoftDev/python/roombox'
